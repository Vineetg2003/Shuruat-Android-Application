<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile"
    android:background="@drawable/main_img2">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_118sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:fontFamily="serif-monospace"
                android:text="@string/SHURUT_APP"
                android:textSize="@dimen/_16ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.066"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.003" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="229dp"
                android:layout_height="186dp"
                android:layout_marginLeft="@dimen/_80sdp"
                android:layout_marginTop="@dimen/_25sdp"
                app:srcCompat="@drawable/profile3" />


            <TextView
                android:id="@+id/editTextTextPersonName13"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:text="Your Name"
                android:drawableLeft="@drawable/baseline_person_24"
                android:textSize="@dimen/_15ssp" />

            <TextView
                android:id="@+id/editTextTextPersonName3"
                android:drawableLeft="@drawable/baseline_date_range_24"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:hint="Date of Birth"
                android:textSize="@dimen/_15ssp"/>
            <TextView
                android:id="@+id/editTextTextPersonName15"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:hint="Contact Number"
                android:textSize="@dimen/_15ssp"
                android:drawableLeft="@drawable/baseline_call_24"/>

            <TextView
                android:id="@+id/editTextTextPersonName16"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:hint="Email Id"
                android:textSize="@dimen/_15ssp"
                android:drawableLeft="@drawable/baseline_email_24"/>
            <TextView
                android:id="@+id/aadhar_number"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:ems="10"
                android:textSize="@dimen/_15ssp"
                android:hint="Aadhar Number" />

            <TextView
                android:id="@+id/father_name"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:ems="10"
                android:textSize="@dimen/_15ssp"
                android:inputType="textPersonName"
                android:hint="Father Name" />
            <TextView
                android:id="@+id/Enroll"
                android:ems="10"
                android:hint="Enrolment Number"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:textSize="@dimen/_15ssp"

                android:drawablePadding="@dimen/_10sdp"/>
            <TextView
                android:id="@+id/annual"
                android:layout_width="312dp"
                android:layout_height="43dp"
                android:layout_marginStart="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_25sdp"
                android:background="@drawable/input_bg2"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingTop="@dimen/_5sdp"
                android:paddingRight="@dimen/_5sdp"
                android:layout_marginBottom="@dimen/_20sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:drawablePadding="@dimen/_10sdp"
                android:ems="10"
                android:inputType="textPersonName"
                android:textSize="@dimen/_15ssp"
                android:hint="Annual Household Income"/>

            <Button
                android:id="@+id/button2"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_40sdp"
                android:text="LOGOUT"
                android:layout_marginBottom="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_200sdp"

                />

            <com.google.android.material.bottomappbar.BottomAppBar
                android:id="@+id/bottomAppBar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:backgroundTint="#6A4063C5"
                app:fabCradleMargin="10dp"
                app:fabCradleRoundedCornerRadius="10dp"
                app:fabCradleVerticalOffset="10dp"
                tools:ignore="BottomAppBar">
                <com.google.android.material.bottomnavigation.BottomNavigationView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_45sdp"
                    android:id="@+id/bottomNavigationView"
                    android:layout_marginRight="16dp"
                    app:menu="@menu/bottom_profile"
                    android:background="@drawable/transparent_background"
                    />
            </com.google.android.material.bottomappbar.BottomAppBar>


        </LinearLayout>
    </ScrollView>







</androidx.coordinatorlayout.widget.CoordinatorLayout>